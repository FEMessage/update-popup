{
  "name": "@femessage/update-popup",
  "version": "1.0.1",
  "description": "Display an update message at right bottom",
  "author": "https://github.com/FEMessage",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FEMessage/update-popup.git"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform",
    "vue",
    "sfc",
    "component",
    "update"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./nuxt": {
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "test": "jest --verbose",
    "test:update": "jest -u",
    "stdver": "standard-version -m '[skip ci] chore(release): v%s'",
    "release": "gren release --override",
    "build": "rimraf dist && tsup 'src/*.{ts,js}' --dts --format cjs,esm",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "start": "esno src/index.ts"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.1",
    "fs-extra": "^9.0.1",
    "unplugin": "^0.2.19",
    "vercel-toast": "^1.4.0"
  },
  "devDependencies": {
    "@femessage/github-release-notes": "^0.19.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "esno": "^0.9.1",
    "fast-glob": "^3.2.7",
    "husky": "1.3.1",
    "jest": "^27.1.0",
    "lint-staged": "^8.1.0",
    "nodemon": "^2.0.12",
    "prettier": "1.18.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "standard-version": "^6.0.1",
    "ts-jest": "^27.0.5",
    "tsup": "^4.14.0",
    "typescript": "^4.4.2",
    "vite": "^2.5.3",
    "webpack": "^5.51.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "vue-sfc-cli": "1.11.4",
  "engines": {
    "node": ">= 8.3.0",
    "npm": ">= 3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.@(md|json)": [
      "prettier --write",
      "git add"
    ],
    "*.[tj]s": [
      "prettier --write",
      "git add"
    ]
  },
  "gren": "@femessage/grenrc",
  "packageManager": "yarn@1.22.19"
}
